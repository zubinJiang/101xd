body,div,p,span,label,ul,ol,li,dl,dd,dt,input,h1,h2,h3,h4,h5,h6{margin:0;padding:0}
body{font-size:12px;font-family:Arial,"宋体";text-align:left;background:url(../../../images/qdsbg_4.gif) repeat-x;color:#434343}
img{border:none}
ul,ol,li{list-style:none}
a{text-decoration:none}a:hover{text-decoration:underline}
.clear{clear:both}

#box{margin:0 auto;width:970px;}
#top{width:970px;height:35px;padding-top:5px}
#top input{width:159px;height:30px;background:url(../../../images/qdsbg_3.gif) no-repeat;line-height:30px;float:right;color:#fff;font-weight:bold;border:none}
#banner{width:964px;height:294px;margin-bottom:5px;padding:4px 0 0 4px;font-family:"黑体";border:1px solid #cfcfcf;background:#fff}
#step{width:966px;height:81px;margin-bottom:5px;background:#fff;border:2px solid #88bf1f;position:relative;padding-top:35px}
#step .hd{width:941px;height:42px;background:url(../../../images/qdsbg_2.gif) no-repeat left;padding-left:27px;line-height:35px;font-size:14px;color:#649900;font-weight:bold;position:absolute;left:-6px;top:5px;}
#step .fd{width:953px;height:63px;padding:12px 0 0 15px;text-align:center;font-size:14px;}
.stepys{width:200px;height:60px;float:left;background:url(../../../images/ddtsbg_4.gif) no-repeat left top;}
#step .fd span{width:200px;height:30px;float:left;line-height:30px;color:#fff;font-weight:bold}
.stephs{width:200px;height:60px;float:left;background:url(../../../images/ddtsbg_4.gif) no-repeat left bottom;}
.aw{width:36px;height:15px;float:left;background:url(../../../images/qdsbg_1.gif) no-repeat center top;margin-top:25px;padding:0 5px 0 5px}
.aw1{width:36px;height:15px;float:left;background:url(../../../images/qdsbg_1.gif) no-repeat center bottom;margin-top:25px;padding:0 5px 0 5px}
#content{width:968px;margin-bottom:5px;background:#fff;border:1px solid #cfcfcf;padding-bottom:10px}
#content .sd{width:855px;height:44px;background:url(../../../images/ddtsbg_2.gif) repeat-x left;line-height:44px;font-size:14px;color:#000;font-weight:bold;font-size:16px;text-align:center;margin-left:63px;background:url(../../../images/ddtsbg_11.gif) no-repeat bottom;border-bottom:1px dashed #999;clear:both}
#content .yi{width:855px;height:104px;line-height:35px;font-size:14px;color:#000;margin-left:63px;}
#content .yi p{text-indent:25px;float:left;width:855px;padding:0;margin:0}
#content .yi p.red{text-indent:25px;color:#f00;float:left;width:855px;padding:0;margin:0}
#tuijian{width:968px;margin-bottom:5px;background:#fff;border:1px solid #cfcfcf;}
#tuijian .hd{width:945px;height:37px;line-height:34px;padding-left:23px;background:url(../../../images/ddtsbg_12.gif) repeat-x left bottom;font-size:14px;font-weight:bold;color:#000;position:relative;margin-bottom:5px}
#tuijian .hd .dw{width:135px;height:40px;position:absolute;left:-9px;top:1px;background:url(../../../images/ddtsbg_13.gif) no-repeat left;color:#fff;padding-left:44px;line-height:38px}
#tuijian .fd{width:943px;padding:7px 0 0 25px;height:145px}
#tuijian .fd p{width:127px;height:62px;margin:0 25px 10px 0;float:left}
.four{width:639px;height:35px;margin-top:8px;padding-left:315px}
.four input{float:left;width:158px;height:33px;background:url(../../../images/ddtsbg_7.gif) no-repeat bottom;font-size:14px;font-weight:bold;color:#fff;border:none;cursor:pointer;margin-right:30px}

#footer{width:968px;height:172px;border:1px solid #d3d2d2;border-bottom:none;background:#fff}
#footer .left{width:228px;height:172px;float:left;background:url(../../../images/logo.gif) no-repeat center;}
#footer .center{width:488px;height:130px;float:left;background:#f3f3f3;padding-top:42px;text-align:center;line-height:30px;color:#666}
#footer .center a{color:#666}
#footer .right{width:236px;height:160px;float:left;background:url(../../../images/showbg_4.gif) no-repeat center;padding:12px 0 0 12px;}
#footer .right .hd{width:220px;height:42px;text-align:center;color:#fff;line-height:19px;padding-top:5px;background:none}
#footer .right .hd span{float:none}
#footer .right .fd{width:190px;height:97px;padding:3px 0 0 23px;line-height:19px}
