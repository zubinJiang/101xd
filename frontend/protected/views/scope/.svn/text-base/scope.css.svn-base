#city{width:968px;overflow:hidden;border:1px solid #cdcec9;}
.tical{float:left;width:968px;line-height:34px;background:url(../../../images/scope_t.gif) no-repeat;}
.tical strong{float:left;width:96px;text-align:center;font-size:15px;color:#be091a;}
.tical span{float:left;width:60px;text-align:center;font-size:14px;}
.tical a{float:left;padding:0 10px;font-size:14px;color:#000;cursor:pointer;outline:medium none;}
.tical a.covert{float:right;font-weight:bold;padding-right:15px;}
.city_name{float:left;width:960px;padding-left:8px;line-height:30px;}
.city_name span{float:left;width:85px;font-size:13px;}
.city_name a{float:left;width:75px;color:#000;font-size:14px;cursor:pointer;font-weight:500;}
#city p{float:left;width:953px;}
#city p a{float:right;padding-left:22px;color:#123cb3;background:url(../../../images/scope.gif) no-repeat 0 6px;line-height:26px;outline:none;}
#city .zifu{float:left;width:780px;}
#city .city_other{text-align:center;font-size:14px;padding-bottom:5px;}
#city .now {text-decoration:none;color:#ff4E00;font-weight:bold;}
