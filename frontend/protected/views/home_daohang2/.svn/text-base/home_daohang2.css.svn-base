.concern{width:920px;padding-left:42px;line-height:30px;background:url(../../../images/concern.gif) no-repeat top left;font-size:14px;font-family:Arial, Helvetica, sans-serif;color:#505050;overflow:hidden;margin-left:8px;}
.concern a{color:#9e0011;float:none;}
