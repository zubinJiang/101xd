#merchant{float:left;width:232px;border:1px solid #8696a5;padding:0 0 10px 4px}
#merchant h1{float:left;width:228px;line-height:30px;background:url(../../../images/merchant.gif) no-repeat top left;border-bottom:3px solid #c7e6eb}
#merchant h1 span{float:left;text-indent:30px;font-size:14px}
#merchant h1 a{float:right;font-size:12px;font-weight:normal;color:#005c9c;padding-right:6px}
#merchant p{float:left;width:228px;text-align:center;line-height:36px;font-size:14px;color:#2b2b2b}
#merchant ul{float:left;width:210px;padding-left:22px;line-height:30px;background:url(../../../images/merchant_ul.gif) no-repeat top left}
#merchant ul li a{color:#51577b;font-size:12px}

