#fastsupply{float:right;width:733px;border:1px solid #c9181e;padding:10px;}
#fastsupply p{float:left;width:726px;padding-left:5px;border:1px solid #babcbc;line-height:27px;height:27px;text-align:center;}
#fastsupply p a{color:#0000ee;}
.audit_90, .audit90{float:left;width:70px;text-align:left;}
.audit235{float:left;width:280px;padding:0 5px 5px 0;text-align:left;display:block;}
#fastsupply ul{float:left;width:726px;padding:5px 0 5px 5px;}
#fastsupply ul li{float:left;width:726px;line-height:24px;}
#fastsupply ul li a{color:#0000ee;}
#fastsupply div.page{float:left;width:726px;padding-left:5px;border:1px solid #babcbc;line-height:27px;height:27px;text-align:center;}
#fastsupply div.page a{color:#0000ee;}

#fastsupply .audit235 a {color:#444;}

#detail {border:2px solid #333;width:346px;height:346px;padding:0;background: #F2F2F2;}
#detail ul {list-style:none;float:left;padding:0;margin:20px 0 0 50px;font-size:12px;width:70%;_margin:20px 0 0 30px;}
#detail ul li {clear:both;margin:0;padding:0;line-height:30px;text-align:left;}
#detail ul li label{width:60px;float:left;text-align:right;color:red;}

/* Overlay */
#contact-overlay {background-color:#444; cursor:wait;}
/* Container */
#contact-container {font: 16px/22px 'Trebuchet MS', Verdana, Arial; text-align:left;width:350px;height:350px;}

#contact-container a.modal-close,
#contact-container a.modal-close:link,
#contact-container a.modal-close:active,
#contact-container a.modal-close:visited {color:#999; font-size:18px; font-weight:bold; position:absolute; text-decoration:none; right:10px; top:3px;}
#contact-container a.modal-close:hover {color:#ccc;}
