$(document).ready(function(){

      $("#releasenetsother .part").click(function(){  
           $("#releasenetsother #cityname").show();
      });

      $("#releasenetsother .tmp").click(function(){    
           $("#releasenetsother .mess_date").show();     
      });

      $("#releasenetsother .agent").click(function(){  
           $("#releasenetsother .mess_agent").show();   
      });

      $("#releasenetsother .identity").click(function(){   
           $("#releasenetsother .mess_agent").hide();   
      });

      $("#releasenetsother .fast").click(function(){   
           $("#releasenetsother .mess_date").hide();   
      });

      $("#releasenetsother .mailing").click(function(){
           $("#releasenetsother #cityname").hide();
                 
      });

      $("#releasenetsother .way2").click(function(){   
           $("#releasenetsother .express").show();       
      });

      $("#releasenetsother .way1").click(function(){   
           $("#releasenetsother .express").hide();       
      });

      $('#releasenetsother #start_date').datepicker({
            inline: true,
            dateFormat:'yy-mm-dd'
      });

      $('#releasenetsother #line_date').datepicker({
            inline: true,
            dateFormat:'yy-mm-dd'
     });


});
